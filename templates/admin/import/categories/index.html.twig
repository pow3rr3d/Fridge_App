{% extends 'admin.html.twig' %}

{% block title %}Import Categories index{% endblock %}

{% block body %}
    <div class="text-center">
        <h1>Import Categories index</h1>
    </div>
    <section class="mt-5">
        {{ form_start(form) }}
        {{ form_row(form.file) }}
        <div class="text-right mt-5">
            <button class="btn btn-primary">{{ button_label|default('Import') }}</button>
        </div>
        {{ form_end(form) }}
    </section>

{% endblock %}
